<h2>Upload a new song</h2>

<%= form_for @music do |f|%>
<%= f.label :artist_name %>
<%= f.text_field :artist_name %>
<br>
<br>
<%= f.label :Composer_name %>
<%= f.text_field :composer %>
<br>
<br>
<%= f.label :Song_file %>
<%= f.file_field :song_file %>
<br>
<br>
<%= f.submit %>
<% end %>

<%= link_to 'Back', musics_path %>